    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.25" RightIndent="7.82" SpaceBefore="0" Spacing="1" StartsNewPage="No" Type="Action" id="5d52b3cc-dcd0-4ab9-a860-f79bfe6a56d6">
      <Text Style="">Kdo je to?</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.13" RightIndent="7.75" SpaceBefore="12" Spacing="1" StartsNewPage="No" Type="Scene Heading" id="edae7a34-d9ae-4c6b-aae1-cbaaf00b5ac5">
      <SceneProperties Length="1/8" Page="7" Title="">
        <SceneArcBeats/>
      </SceneProperties>
      <Text Style="Bold+AllCaps">ŠVANDA</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.25" RightIndent="7.82" SpaceBefore="0" Spacing="1" StartsNewPage="No" Type="Action" id="b54a6ff6-6059-4b8e-af86-5021217e1d4c">
      <Text Style="">Já. Pojď ven!</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.13" RightIndent="7.75" SpaceBefore="12" Spacing="1" StartsNewPage="No" Type="Scene Heading" id="015bbe27-85fb-4082-a17b-684476f20bb8">
