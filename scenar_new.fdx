      <Text Style="">Tady a Andělé? Leda tohle smetí.</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.13" RightIndent="7.75" SpaceBefore="12" Spacing="1" StartsNewPage="No" Type="Scene Heading" id="7718b207-d4eb-47ad-ab5a-4fa39ed14fb2">
