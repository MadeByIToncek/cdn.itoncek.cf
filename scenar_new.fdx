        <SceneArcBeats/>
      </SceneProperties>
      <Text Style="Bold+AllCaps">DorOTKA</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.25" RightIndent="7.82" SpaceBefore="0" Spacing="1" StartsNewPage="No" Type="Action" id="141048e5-d4ac-4461-aeca-dc8a0392f985">
      <Text Style="">A kam jdeš ty?</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.13" RightIndent="7.75" SpaceBefore="12" Spacing="1" StartsNewPage="No" Type="Scene Heading" id="66b14be2-fb7b-4043-8031-f3e7bcb0f882">
