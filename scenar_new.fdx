      </SceneProperties>
      <Text Style="Bold+AllCaps">ŠVANDA</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.25" RightIndent="7.82" SpaceBefore="0" Spacing="1" StartsNewPage="No" Type="Action" id="af4d7534-4e36-4e10-b185-56ef92b97147">
      <Text Style="">Ale co bych nevyhrál, naleštím si benjo a ulíznu si patku.</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.13" RightIndent="7.75" SpaceBefore="12" Spacing="1" StartsNewPage="No" Type="Scene Heading" id="94234fe9-38b9-4b25-9760-90c9de1312f7">
