      <Text Style="Italic">(přistoupí k Švandovi)</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.25" RightIndent="7.82" SpaceBefore="0" Spacing="1" StartsNewPage="No" Type="Action" id="a7bb6e3f-daaf-448b-9f17-780b6e010c35">
      <Text Style="">Líbí se ti něco, pane? </Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.13" RightIndent="7.75" SpaceBefore="12" Spacing="1" StartsNewPage="No" Type="Scene Heading" id="0c1275fb-23d9-479f-9ceb-3887abf43ae8">
