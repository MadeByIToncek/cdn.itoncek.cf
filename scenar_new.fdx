      </SceneProperties>
      <Text Style="Bold+AllCaps">PoROTCE 3</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.25" RightIndent="7.82" SpaceBefore="0" Spacing="1" StartsNewPage="No" Type="Action" id="c6cd43d1-fff3-4f7a-9896-ff33f0a50f76">
      <Text Style="">Nejsem úplně fanoušek rapu ale váš talent se popřít nedá takže za mě jdete určitě dál. </Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.13" RightIndent="7.75" SpaceBefore="12" Spacing="1" StartsNewPage="No" Type="Scene Heading" id="068c8065-6dbb-4802-b4f5-f21e30c53c88">
