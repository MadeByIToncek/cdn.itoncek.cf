      <Text Style="">Já taky, zlatíčko. Čtrnáct dní jsem si šetřil žízeň. </Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.13" RightIndent="7.75" SpaceBefore="12" Spacing="1" StartsNewPage="No" Type="Scene Heading" id="f8a27292-f38f-4b15-b388-0600b16a46a0">
