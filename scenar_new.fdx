      </SceneProperties>
      <Text Style="Bold+AllCaps">DOROTKA</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.25" RightIndent="7.82" SpaceBefore="0" Spacing="1" StartsNewPage="No" Type="Action" id="cd2cc53d-0cf6-4f85-9e9b-66f1a946aa3c">
      <Text Style="">Copak je to?</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.13" RightIndent="7.75" SpaceBefore="12" Spacing="1" StartsNewPage="No" Type="Scene Heading" id="ebf80c76-576c-49d3-bb30-7f2c1300ba99">
