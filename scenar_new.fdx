        <SceneArcBeats/>
      </SceneProperties>
      <Text Style="Bold+AllCaps">ŠVANDA</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.25" RightIndent="7.82" SpaceBefore="0" Spacing="1" StartsNewPage="No" Type="Action" id="5c03957d-f80f-475c-9ad5-86ac17b5bebf">
      <Text Style="">Já vím, co udělám, aby tvůj tatík změnil názor na mne.</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.13" RightIndent="7.75" SpaceBefore="12" Spacing="1" StartsNewPage="No" Type="Scene Heading" id="20602db0-119c-4af2-a3e5-3bdcf325974b">
