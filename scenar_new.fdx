        <SceneArcBeats/>
      </SceneProperties>
      <Text Style="Bold+AllCaps">ŠvANDA</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.25" RightIndent="7.82" SpaceBefore="0" Spacing="1" StartsNewPage="No" Type="Action" id="a128325d-74ec-4a61-8756-49ed35a4bcd9">
      <Text Style="">Jdi sám – nemám náladu – mně je tuhle</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="-0.10" Leading="Regular" LeftIndent="2.75" RightIndent="5.82" SpaceBefore="0" Spacing="1" StartsNewPage="No" Type="Character" id="cebb33ce-df30-452f-9b43-793b00a7bee0">
      <Text Style="Italic">(sahá si na srdce)</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.25" RightIndent="7.82" SpaceBefore="0" Spacing="1" StartsNewPage="No" Type="Action" id="e8f80fb7-f5dd-4a6f-9c44-d68b0d2cdfe8">
      <Text Style="">jako by mi chtělo srdce prasknout. Neslyšel jsi, že má jiný za muziku tisíce?</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Type="Action" id="3ab49f6d-ab41-49d5-a3d0-55e774a902d3">
      <Text>(vyklepe prázdnou peněženku)</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.13" RightIndent="7.75" SpaceBefore="12" Spacing="1" StartsNewPage="No" Type="Scene Heading" id="0df73280-fbdc-44cc-b37b-1d7e784a1fa6">
