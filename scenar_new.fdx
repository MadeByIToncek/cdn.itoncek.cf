        <SceneArcBeats/>
      </SceneProperties>
      <Text Style="Bold+AllCaps">KaLAFUNA</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.25" RightIndent="7.82" SpaceBefore="0" Spacing="1" StartsNewPage="No" Type="Action" id="b3c489b6-1a66-43d3-87e6-edcd0b38a527">
      <Text Style="">Což o to! Švanda to umí z fleku; dnes ste to zrovna vidět nemoh, když mu láska tlačí na srdíčko – he he! – a z toho má potom zamotané prsty se strunama. </Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.13" RightIndent="7.75" SpaceBefore="12" Spacing="1" StartsNewPage="No" Type="Scene Heading" id="bee3ec2c-a126-4803-94d8-8c24c787b6b3">
