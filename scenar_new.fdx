      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.25" RightIndent="7.82" SpaceBefore="0" Spacing="1" StartsNewPage="No" Type="Action" id="6fba9f3a-dbc6-4a03-8ae9-bb78973012a2">
      <Text Style="">Tatínku to jsem já, Zulika. </Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.13" RightIndent="7.75" SpaceBefore="12" Spacing="1" StartsNewPage="No" Type="Scene Heading" id="5a6cf1e4-2344-484d-801e-349f0e65fc62">
      <SceneProperties Length="1/8" Page="28" Title="">
        <SceneArcBeats/>
      </SceneProperties>
      <Text Style="Bold+AllCaps">BOSS</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.25" RightIndent="7.82" SpaceBefore="0" Spacing="1" StartsNewPage="No" Type="Action" id="465b36fb-ffca-460d-95c2-cba937298985">
      <Text Style="">Ty?! A co to je za koště s tebou, a co tady děláš v tomhle stavu? a co jste spolu dělali?</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.13" RightIndent="7.75" SpaceBefore="12" Spacing="1" StartsNewPage="No" Type="Scene Heading" id="169e0470-3018-42f4-b5ee-3a33f7d50e45">
      <SceneProperties Length="1/8" Page="28" Title="">
        <SceneArcBeats/>
      </SceneProperties>
      <Text Style="Bold+AllCaps">ZULIKA</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.25" RightIndent="7.82" SpaceBefore="0" Spacing="1" StartsNewPage="No" Type="Action" id="fd94f60f-14a4-4977-8d24-1c63946bd265">
      <Text Style="">To nic tatínku, jdi spát. Popovídáme si ráno.</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.13" RightIndent="7.75" SpaceBefore="12" Spacing="1" StartsNewPage="No" Type="Scene Heading" id="108628ff-29db-4539-809a-aab93acd6211">
