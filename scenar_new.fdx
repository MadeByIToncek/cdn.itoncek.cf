      <Text Style="">Stará!</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.13" RightIndent="7.75" SpaceBefore="12" Spacing="1" StartsNewPage="No" Type="Scene Heading" id="9c670652-d7a7-4b04-b011-826bde36439e">
