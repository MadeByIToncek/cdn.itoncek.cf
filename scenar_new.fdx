        <SceneArcBeats/>
      </SceneProperties>
      <Text Style="Bold+AllCaps">KALAFUNA</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.25" RightIndent="7.82" SpaceBefore="0" Spacing="1" StartsNewPage="No" Type="Action" id="951c4841-d423-4087-8431-6317cf7293b1">
      <Text Style="">Měla, měla– ale žádná to nepozná před svatbou, každá teprve deset let potom – když se nabaží. Jó, vy ženské.</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="3.25" RightIndent="7.13" SpaceBefore="24" Spacing="1" StartsNewPage="No" Type="Dialogue" id="dab6da76-8b1e-4328-86f7-f9f932023b9b">
      <Text Style="AllCaps">Výstup 5 - křižovatka u hájovny</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Type="Action" id="221a458d-41af-4804-9bbc-146ba0069119">
      <Text>Švanda, potom Dorotka. </Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.13" RightIndent="7.75" SpaceBefore="12" Spacing="1" StartsNewPage="No" Type="Scene Heading" id="be50bc7b-d244-4591-b70c-bc292894b35d">
      <SceneProperties Length="1/8" Page="7" Title="">
