      <Text Style="">Tak jdi – my máme dost, hehe, když nás předseda a pole živí.</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.13" RightIndent="7.75" SpaceBefore="12" Spacing="1" StartsNewPage="No" Type="Scene Heading" id="cd213980-80a5-4e6b-8d03-e0f4d6f48ff6">
      <SceneProperties Length="1/8" Page="4" Title="">
        <SceneArcBeats/>
      </SceneProperties>
      <Text Style="Bold+AllCaps">ŠVANDA</Text>
      <UserParagraphData>
					
