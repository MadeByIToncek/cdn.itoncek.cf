      <SceneProperties Length="1/8" Page="28" Title="">
        <SceneArcBeats/>
      </SceneProperties>
      <Text>VÝSTUP 8 - MAFIÁNSKÉ SÍDLO</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Type="Action" id="6175ad45-2698-4ada-9214-a12f3bf13246">
      <Text>švanda a zulika vylezou na potemnělé podium, rozcuchaní, od rtěnky a celkově neupravení</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.13" RightIndent="7.75" SpaceBefore="12" Spacing="1" StartsNewPage="No" Type="Scene Heading" id="05a2c2fb-0138-4b28-9783-bbe50c94d2da">
      <SceneProperties Length="1/8" Page="28" Title="">
        <SceneArcBeats/>
      </SceneProperties>
      <Text Style="Bold+AllCaps">Boss</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="-0.10" Leading="Regular" LeftIndent="2.75" RightIndent="5.82" SpaceBefore="0" Spacing="1" StartsNewPage="No" Type="Character" id="2cb7f7f2-ac33-4eb4-a08f-c191ebf047fc">
      <Text Style="Italic">(v župánku se vypotácí zpoza opony se sklenkou whisky)</Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.25" RightIndent="7.82" SpaceBefore="0" Spacing="1" StartsNewPage="No" Type="Action" id="b6220c39-8046-479b-a245-6d0781bc85e4">
      <Text Style="">Co je to tady za randál? Párty zkončila, vypadněte! </Text>
      <UserParagraphData>
					
				</UserParagraphData>
    </Paragraph>
    <Paragraph Alignment="Left" FirstIndent="0.00" Leading="Regular" LeftIndent="1.13" RightIndent="7.75" SpaceBefore="12" Spacing="1" StartsNewPage="No" Type="Scene Heading" id="89c06b0b-7679-4acb-8b88-f1679e16865b">
      <SceneProperties Length="1/8" Page="28" Title="">
